Id : (A:Type) -> A -> A -> Type;
refl : (A:Type) -> (a:A) -> Id A a a;

J : (A:Type) -> (P:(a:A) -> (b:A) -> Id A a b -> Type) 
  -> ((a:A) -> P a a (refl A a))
  -> (a:A) -> (b:A) -> (p : Id A a b) -> P a b p;

forceEq : (A:Type) -> (a:^A) -> Id (^A) a [!a];
